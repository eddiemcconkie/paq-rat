{
	"arrowParens": "always",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-css-order"],
	"printWidth": 100,
	"quoteProps": "as-needed",
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all",
	"useTabs": true
}
