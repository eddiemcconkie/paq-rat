{
  "name": "box-tags",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "db:start": "bun db/scripts/start.ts",
    "db:create": "bun db/scripts/create-db.ts",
    "db:sql": "surreal sql --ns $PUBLIC_SURREAL_NS --db $PUBLIC_SURREAL_DB --pretty",
    "db:migrate": "bun db/scripts/migrate.ts",
    "db:create-migration": "bun db/scripts/create-migration.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/bun": "latest",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "arctic": "^0.10.4",
    "googleapis": "^131.0.0",
    "lucia": "beta",
    "oslo": "^0.27.1",
    "surrealdb.js": "^0.11.0",
    "zod": "^3.22.4"
  }
}
